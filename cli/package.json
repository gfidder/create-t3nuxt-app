{
  "name": "create-t3nuxt-app",
  "version": "0.2.0",
  "description": "Create web application with the t3 Nuxt stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gfidder/create-t3nuxt-app.git",
    "directory": "cli"
  },
  "keywords": [
    "create-t3nuxt-app",
    "nuxt",
    "t3-stack",
    "tailwind",
    "tRPC",
    "typescript"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-t3nuxt-app": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write **/*.{cjs,mjs,ts,tsx,md,json,vue} --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern",
    "format:check": "prettier --check **/*.{cjs,mjs,ts,tsx,md,json,vue} --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern",
    "release": "changeset version",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "execa": "^6.1.0",
    "fs-extra": "^11.1.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.1.4",
    "ora": "^6.1.2",
    "sort-package-json": "^2.6.0"
  },
  "devDependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.9.0",
    "@sidebase/nuxt-auth": "^0.4.1",
    "@trpc/client": "^10.10.0",
    "@trpc/server": "^10.10.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.2.5",
    "eslint-plugin-isaacscript": "^2.3.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "next-auth": "4.18.8",
    "nuxt": "^3.7.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^4.9.0",
    "superjson": "^1.12.2",
    "trpc-nuxt": "^0.5.0",
    "tsup": "^7.2.0",
    "type-fest": "^3.5.6",
    "typescript": "^5.0.4",
    "vite-plugin-eslint": "^1.8.1",
    "zod": "^3.20.2"
  }
}
